{
	"rules": {
		"users": {
			"$uid": {
				".read": "$uid === auth.uid",
				".write": "$uid === auth.uid",
				"apikey": {
					".read": "$uid === auth.uid",
					".write": "$uid === auth.uid"
				},
				"profile": {
					".read": true,
					".write": "$uid === auth.uid",
					"account": {
						".read": true,
						".write": "$uid === auth.uid",
						"name": {},
						"age": {},
						"ap": {},
						"fractal": {},
						"wvw": {}
					}
				}
			}
		}
	}
}